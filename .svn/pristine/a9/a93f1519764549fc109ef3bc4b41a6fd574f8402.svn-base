<form name="userForm2" action="/SpringMVCDemo/fileupload/upload" enctype="multipart/form-data" method="post"">
<div id="newUpload2">
    <input type="file" name="file">
</div>
<input type="button" id="btn_add2" value="增加一行" >
<input type="submit" value="上传" >


</form>
<script type="text/javascript">
    j = 1;
    $(document).ready(function(){


        $("#btn_add2").click(function(){
            document.getElementById("newUpload2").innerHTML+='<div id="div_'+j+'"><input  name="file_'+j+'" type="file"  /><input type="button" value="删除"  onclick="del_2('+j+')"/></div>';
            j = j + 1;
        });
    });

    function del_2(o){
        document.getElementById("newUpload2").removeChild(document.getElementById("div_"+o));
    }

</script>